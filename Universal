spawn(function()
 local hi = Instance.new("Sound")
hi.Name = "Sound"
hi.SoundId = "http://www.roblox.com/asset/?id=1216343787"
hi.Volume = 10
hi.Looped = false
hi.archivable = false
hi.Parent = game.Workspace

if game.Players.LocalPlayer.Character.Humanoid.Health > 10 then
hi:Play()
end
end)

spawn(function()
 local hi = Instance.new("Sound")
hi.Name = "Sound"
hi.SoundId = "http://www.roblox.com/asset/?id=1677094219"
hi.Volume = 5
hi.Looped = true
hi.archivable = false
hi.Parent = game.Workspace
if game.Players.LocalPlayer.Character.Humanoid.Health > 10 then
hi:Play()
end
end)

spawn(function()
game:GetService'Players'.LocalPlayer.CharacterAdded:Connect(function(Character)
wait(3)
 local hi = Instance.new("Sound")
hi.Name = "Sound"
hi.SoundId = "http://www.roblox.com/asset/?id=1216343787"
hi.Volume = 10
hi.Looped = false
hi.archivable = false
hi.Parent = game.Workspace
if game.Players.LocalPlayer.Character.Humanoid.Health > 10 then
hi:Play()
end
end)
end)
